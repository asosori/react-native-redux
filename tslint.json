{
    "rulesDirectory": ["tslint-plugin-prettier"],
    "extends": ["tslint-react", "tslint-config-prettier"],
    "rules": {
      "jsx-boolean-value": [true, "never"],
      "jsx-no-lambda": false,
      "prettier": [
        true, {
          "singleQuote": true,
          "semi": true
        }
      ]
    }
  }